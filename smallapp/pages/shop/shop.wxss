.classification {
  display: flex;
}

.list {
  width: 176rpx;
}

.list_name {
  padding: 26rpx;
  text-align: center;
  font-size: 28rpx;
  color: #696969;
}

.data {
  height: 1080rpx;
  flex: 1;
}

.data_img {
  width: 120rpx;
  height: 120rpx;
}

.main {
  overflow: hidden;
  background-color: #fff;
}

.main_list {
  display: flex;
  margin: 10rpx 0;
}

.main_img {
  width: 170rpx;
  height: 170rpx;
}
.item {
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
  flex: 1;
  justify-content: space-around;
  margin-left: 20rpx;
  overflow: hidden;
}
.main_name {
  font-size: 30rpx;
  font-weight: bold;
}
.business{
  color: #a9a9a9;
}
.money{
  color: #cd2d2d;
}

.data_type {
  text-align: center;
  font-size: 26rpx;
  color: #56b9e2;
  margin: 20rpx 0;
}

.active {
  color: #56b9e2;
  box-sizing: border-box;
  background-color: #fff;
}

