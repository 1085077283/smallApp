page{
  height: auto;
}
.list{
  padding:10rpx 0 0 20rpx;
  background: #fff; 
}
.item{
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e9e9e9;
  height: 120rpx;
}
.item:last-child{
  border: none;
}
.icon{
  width: 64rpx;
  height: 64rpx;
  margin: 30rpx 30rpx 30rpx 0;
}
.name{
  font-size: 32rpx;
  color: #050a1d;
  font-weight: 600;
}